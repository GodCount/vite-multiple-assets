{
  "name": "vite-multiple-assets",
  "version": "1.2.10",
  "description": "Multiple assets public directory for vite",
  "author": "Van nguyen ba tran",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/nguyenbatranvan/vite-multiple-assets"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "module": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx lerna run dev",
    "build-example": "npx lerna run build",
    "preview": "npx lerna run preview",
    "build": "tsup ./libs/index.ts --clean --dts --format cjs,esm",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "vite-multiple-assets",
    "Vite",
    "Multiple Assets",
    "Multiple directory"
  ],
  "devDependencies": {
    "@swc/core": "^1.3.72",
    "@types/node": "^20.4.5",
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.1",
    "lerna": "^7.1.4",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "peerDependencies": {
    "mime-types": "^2.1.35",
    "vite": ">=2.9.6"
  }
}
